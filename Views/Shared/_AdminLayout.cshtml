    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="/AdminLayouts/css/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="/AdminLayouts/css/style.css" rel="stylesheet">

    <main id="main" class="main">
        <div class="pagetitle">
            <h1>@ViewBag.PageTitle</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/Admin">Home</a></li>
                    <li class="breadcrumb-item active">@ViewBag.PageTitle</li>
                </ol>
            </nav>
        </div>
        <section class="section">
            @RenderBody()
        </section>
    </main>
